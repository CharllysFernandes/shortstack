{
  "name": "shortstack",
  "version": "1.1.0",
  "author": "Adam Argyle",
  "description": "simple starter",
  "main": "index.js",
  "scripts": {
    "start": "npm run concurrent",
    "concurrent": "concurrently --kill-others \"npm run dev:js\" \"npm run dev:css\" \"npm run dev:server\"",
    "bundle": "rollup -c",
    "dev:js": "rollup -c -w",
    "dev:css": "postcss app/index.css -o app/bundle.css -w",
    "dev:server": "browser-sync start --server 'app' --files 'app/index.html,app/bundle.css,app/bundle.js' --no-open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "blingblingjs": "^2.0.2",
    "ragrid": "^1.0.5"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "concurrently": "^4.1.0",
    "postcss": "^7.0.7",
    "postcss-cli": "^6.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "rollup": "^1.0.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3"
  }
}
